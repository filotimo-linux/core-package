msgid "dnfdragora"
msgstr "dnfdragora"

#: ./share/applications/org.mageia.dnfdragora.desktop
msgctxt "GenericName"
msgid "Install & Remove Software"
msgstr "Instal·leu i elimineu programari"

#: ./share/applications/org.mageia.dnfdragora.desktop
