#: ./share/applications/org.mageia.dnfdragora.desktop
msgctxt "Name"
msgid "dnfdragora"
msgstr "dnfdragora"

#: ./share/applications/org.mageia.dnfdragora.desktop
msgctxt "GenericName"
msgid "Install & Remove Software"
msgstr "Installera och ta bort program"

#: ./share/applications/org.mageia.dnfdragora.desktop
msgctxt "Comment"
msgid "A graphical front end for installing, removing and updating packages"
msgstr ""
"Ett grafiskt gränssnitt för installation, borttagning och uppdatering av "
"paket"

#: ./share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "Name"
msgid "dnfdragora-updater"
msgstr "Uppdatera dnfdragora"

#: ./share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "GenericName"
msgid "Tray-icon to check for updates"
msgstr "Aktivitetsikon för uppdateringskontroll"

#: ./share/applications/org.mageia.dnfdragora-updater.desktop
msgctxt "Comment"
msgid "Automatically check for updates and notify"
msgstr "Kontrollera automatiskt efter uppdateringar och meddela"

#: ./share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "Name"
msgid "dnfdragora Package Installer"
msgstr "Paketinstallerare för dnfdragora"

#: ./share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "GenericName"
msgid "RPM Package Installer"
msgstr "RPM-paketinstallerare"

#: ./share/applications/org.mageia.dnfdragora-localinstall.desktop
msgctxt "Comment"
msgid "Install local packages on the system"
msgstr "Installera lokala paket på systemet."
